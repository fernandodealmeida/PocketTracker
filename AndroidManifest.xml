<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fernando.pockettracker"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="10" />

    <application android:icon="@drawable/cart" android:theme="@android:style/Theme.Light">
		<activity android:name=".CadastroGastos" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.fernando.pockettracker.EditarGasto" android:label="EditarGasto" 
			android:configChanges="orientation|keyboardHidden"
			/>
		<activity android:name="com.fernando.pockettracker.BuscarGasto" android:label="BuscarGasto" />
	</application>

</manifest>
